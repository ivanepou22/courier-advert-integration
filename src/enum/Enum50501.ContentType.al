enum 50501 "Content Type"
{
    Extensible = true;

    value(0; " ") { }
    value(10; "application/json")
    {
        Caption = 'APPLICATION/JSON';
    }
    value(20; "application/xml")
    {
        Caption = 'APPLICATION/XML';
    }
    value(30; "text/plain")
    {
        Caption = 'TEXT/PLAIN';
    }
    value(40; "image/*")
    {
        Caption = 'IMAGE/*';
    }
    value(50; "application/octet-stream")
    {
        Caption = 'APPLICATION/OCTET-STREAM';
    }
}
